{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "storage": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/service"
      }
    }
  },
  "$defs": {
    "service": {
      "type": "object",
      "id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "company": {
        "type": "string"
      }
    }
  },
  "required": ["location", "fuel"]
}
