{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "fuel": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/fuel"
      }
    },
    "logistic": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/logistic"
      }
    },
    "storageRental": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/storageRental"
      }
    },
    "matches": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/match"
      }
    },
    "additionalProperties": false
  },
  "$defs": {
    "fuel": {
      "type": "object",
      "properties": {
        "dispenser": {
          "$ref": "#/$defs/dispenser"
        },
        "producer": {
          "$ref": "#/$defs/producer"
        },
        "service": {
          "$ref": "#/$defs/service"
        },
        "quote": {
          "$ref": "#/$defs/quote"
        }
      },
      "additionalProperties": false
    },
    "logistic": {
      "type": "object",
      "properties": {
        "service": {
          "$ref": "#/$defs/service"
        },
        "vehicle": {
          "$ref": "#/$defs/vehicle"
        },
        "quote": {
          "$ref": "#/$defs/quote"
        }
      },
      "additionalProperties": false
    },
    "storageRental": {
      "type": "object",
      "properties": {
        "service": {
          "$ref": "#/$defs/service"
        },
        "storage": {
          "$ref": "#/$defs/storage"
        },
        "quote": {
          "$ref": "#/$defs/quote"
        }
      },
      "additionalProperties": false
    },
    "match": {
      "type": "object",
      "properties": {
        "fuel": {
          "type": "string"
        },
        "logistic": {
          "type": "string"
        },
        "storage": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "type": {
              "type": "string"
            }
          },
          "required": ["id", "type"]
        },
        "fuelUtilisation": {
          "type": "number"
        },
        "price": {
          "type": "number"
        },
        "transportDistance": {
          "type": "number"
        },
        "CO2e": {
          "type": "number"
        }
      },
      "required": ["fuel", "logistic", "storage", "fuelUtilisation", "transportDistance"],
      "additionalProperties": false
    },
    "storage": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "availableQuantity": {
          "type": "number"
        },
        "capacity": {
          "type": "number"
        }
      },
      "additionalProperties": false
    },
    "vehicle": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "availableQuantity": {
          "type": "number"
        },
        "transportDistance": {
          "type": "number"
        }
      },
      "additionalProperties": false
    },
    "dispenser": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "fillRate": {
          "type": "number"
        },
        "fillingStationCapacity": {
          "type": "number"
        },
        "lat": {
          "type": "number"
        },
        "long": {
          "type": "number"
        }
      },
      "additionalProperties": false
    },
    "producer": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "dailyOfftakeCapacity": {
          "type": "number"
        },
        "productionCO2e": {
          "type": "number"
        }
      },
      "additionalProperties": false
    },
    "service": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "transportCO2e": {
          "type": "number"
        },
        "exclusiveDownstreamCompanies": {
          "type": "string"
        },
        "exclusiveUpstreamCompanies": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "quote": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "monetaryValue": {
          "type": "number"
        }
      },
      "additionalProperties": false
    }
  },
  "required": ["fuel", "logistic", "matches"]
}
